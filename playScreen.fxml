<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="playingPane" prefHeight="715.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalstickhero.playController">
    <children>
        <ImageView fitHeight="715.0" fitWidth="1000.0" pickOnBounds="true">
            <image>
                <Image url="@playBackground.jpeg" />
            </image>
        </ImageView>
        <ImageView fitHeight="44.0" fitWidth="50.0" layoutX="7.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@home_button.png" />
            </image></ImageView>
        <Button layoutX="7.0" layoutY="6.0" mnemonicParsing="false" onAction="#switch2HomeScreen" opacity="0.0" prefHeight="44.0" prefWidth="44.0" />
    </children>
</AnchorPane>
